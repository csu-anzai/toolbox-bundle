{
  "name": "atournayre/toolbox-bundle",
  "description": "ToolboxBundle pour Symfony 4",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Aur√©lien Tournayre",
      "email": "aurelien.tournayre@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Atournayre\\ToolboxBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Atournayre\\ToolboxBundle\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.2.9",
    "nesbot/carbon": "^2.23",
    "phpoffice/phpspreadsheet": "^1.8",
    "google/apiclient": "^2.2",
    "ext-json": "*",
    "symfony/swiftmailer-bundle": "^3.2",
    "beberlei/doctrineextensions": "^1.2",
    "spipu/html2pdf": "^5.2",
    "symfony/property-access": "^4.2",
    "symfony/flex": "^1.4",
    "twig/twig": "^2.11",
    "symfony/process": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "doctrine/orm": "^2.6",
    "symfony/form": "^4.2",
    "symfony/http-foundation": "^4.2",
    "tomsgu/pdf-merger": "^0.1.0",
    "globalcitizen/php-iban": "^2.7",
    "symfony/validator": "4.2.*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.2",
    "behat/behat": "^3.5",
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "infection/infection": "^0.13.4",
    "phpmd/phpmd": "^2.7",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/maker-bundle": "^1.12"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.2.*"
    }
  }
}
